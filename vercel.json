{
    "version": 2,
    "functions": {
        "api/index.ts": {
            "runtime": "@vercel/node",
            "maxDuration": 30
        }
    },
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/api/index.ts"
        }
    ],
    "buildCommand": "cd client && npm run build",
    "outputDirectory": "client/dist"
}